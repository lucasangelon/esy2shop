<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="codefactory.projectshop" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="codefactory.esy2shop.activites.MainMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="codefactory.esy2shop.activites.EditList"
            android:label="@string/title_activity_edit_list" >
        </activity>
        <activity
            android:name="codefactory.esy2shop.activites.CopyrightActivity"
            android:label="@string/title_activity_copyright" >
        </activity>
        <activity
            android:name="codefactory.esy2shop.activites.EditStore"
            android:label="@string/title_activity_edit_store" >
        </activity>
        <activity
            android:name="codefactory.esy2shop.activites.EditNotification"
            android:label="@string/title_activity_edit_notification" >
        </activity>
        <activity
            android:name="codefactory.esy2shop.activites.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
    </application>

</manifest>
